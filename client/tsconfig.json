{
    "compilerOptions": {
        "jsx": "react-jsx",
        "target": "esnext",
        "types": ["vite/client"],
        "allowSyntheticDefaultImports": true,
        "module": "esnext",
        "isolatedModules": true,
        "strict": true,
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "lib": ["esnext", "dom", "dom.iterable"]
    },
    "include": ["src"]
}

